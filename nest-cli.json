{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "backend": {
      "type": "application",
      "root": "src",
      "entryFile": "main",
      "sourceRoot": "src"
    },
    "scheduler": {
      "type": "application",
      "root": "src/apps/scheduler",
      "entryFile": "scheduler",
      "sourceRoot": "src/apps/scheduler"
    },
    "processors": {
      "type": "application",
      "root": "src/apps/processors",
      "entryFile": "processors",
      "sourceRoot": "src/apps/processors"
    }
  }
}
