{
  "name": "squidward-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:scheduler:dev": "NODE_ENV=development INSTANCE_TYPE=scheduler nest start scheduler --watch",
    "start:processors:dev": "NODE_ENV=development INSTANCE_TYPE=processors nest start processors --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=development node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrations:create": "tsx ./src/database/migrations-create",
    "migrations:up": "tsx ./src/database/migrations --up",
    "migrations:down": "tsx ./src/database/migrations --down",
    "migrations:up:prod": "node ./dist/src/database/migrations.js --up",
    "postinstall": "node shell/postinstall.js"
  },
  "dependencies": {
    "@bull-board/api": "^6.13.0",
    "@bull-board/express": "^6.13.0",
    "@bull-board/nestjs": "^6.13.0",
    "@bull-board/ui": "^6.13.0",
    "@keyv/redis": "^5.1.2",
    "@nestjs/bull-shared": "^11.0.3",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@peculiar/webcrypto": "^1.5.0",
    "@peculiar/x509": "^1.14.0",
    "@scalar/nestjs-api-reference": "^1.0.2",
    "axios": "^1.12.1",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.5",
    "cache-manager": "^7.2.2",
    "command-line-args": "^6.0.1",
    "compression": "^1.8.1",
    "consola": "^3.4.2",
    "dayjs": "^1.11.18",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^5.5.2",
    "kysely": "^0.28.7",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.1",
    "nanoid": "^5.1.5",
    "nest-winston": "^1.10.2",
    "nestjs-zod": "^5.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pkg-types": "^2.3.0",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "winston": "^3.17.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6",
    "@types/command-line-args": "^5.2.3",
    "@types/compression": "^1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9",
    "@types/lodash-es": "^4",
    "@types/morgan": "^1",
    "@types/node": "^22.10.7",
    "@types/passport": "^0",
    "@types/passport-jwt": "^4",
    "@types/pg": "^8",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-plugin-paths": "^1.1.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.101.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
