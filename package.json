{
  "name": "squidward-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:scheduler: dev": "NODE_ENV=development nest start scheduler --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=development node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrations:create": "yarn tsx ./src/database/migrations-create",
    "migrations:up": "yarn tsx ./src/database/migrations --up",
    "migrations:down": "yarn tsx ./src/database/migrations --down"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@squidward-node/contracts": "file:../squidward-node/libs/contracts",
    "axios": "^1.12.1",
    "bcrypt": "^6.0.0",
    "command-line-args": "^6.0.1",
    "compression": "^1.8.1",
    "consola": "^3.4.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "kysely": "^0.28.7",
    "lodash-es": "^4.17.21",
    "luxon": "^3.7.2",
    "morgan": "^1.10.1",
    "nest-winston": "^1.10.2",
    "nestjs-zod": "^5.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.17.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6",
    "@types/command-line-args": "^5.2.3",
    "@types/compression": "^1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/lodash-es": "^4",
    "@types/luxon": "^3",
    "@types/morgan": "^1",
    "@types/node": "^22.10.7",
    "@types/pg": "^8",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-plugin-paths": "^1.1.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@4.9.4"
}
